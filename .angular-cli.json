{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "preference-pass"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "icons",
        "favicon.ico",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "serviceWorker": true,
      "styles": [
        "assets/css/preference-pass.scss",
        "assets/css/bootstrap.min.css",
        "assets/scss/app.variables.scss",
        "assets/scss/app.buttons.scss",
        "assets/css/font-awesome.min.css",
        "assets/css/photoswipe/photoswipe.css",
        "assets/css/photoswipe/photoswipe-default-skin.css",
        "assets/froala/css/froala_editor.pkgd.css",
        "assets/froala/css/froala_style.css",
        "assets/css/swiper.min.css",
        "styles.scss"
      ],
      "scripts": [
        "vendors/js/auth0-7.6.1.min.js",
        "assets/js/photoswipe/photoswipe.min.js",
        "assets/js/photoswipe/photoswipe-ui-default.min.js",
        "assets/froala/js/froala_editor.pkgd.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {
    }
  }
}
